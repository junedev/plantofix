<h2> You can login here:</h2>
<%= form_tag sessions_path do %>
<div class="field">
  <%= label_tag :email %>
  <%= text_field_tag :email %>
</div>
<div class="field">
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</div>
<div class="actions"><%= submit_tag "Log me in" %></div>
<% end %>
